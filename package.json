{
  "name": "libp2p-observation-deck",
  "version": "0.0.1",
  "description": "Public catalogue of widgets for visualising LibP2P introspection data, built on libp2p-observer",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/libp2p-observation-deck.git"
  },
  "author": "Nearform",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nearform/libp2p-observation-deck/issues"
  },
  "homepage": "https://github.com/nearform/libp2p-observation-deck#readme",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "CI=true craco test",
    "test:watch": "craco test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "peerDependencies": {
    "@nearform/observer-data": "^0.0.1",
    "@nearform/observer-shell": "^0.0.1",
    "@nearform/observer-sdk": "^0.0.1",
    "webpack": "^4.41.0"
  },
  "dependencies": {
    "@baristalabs/craco-raw-loader": "^1.1.1",
    "@craco/craco": "^5.6.4",
    "@nearform/observer-catalogue": "^0.0.2",
    "@nearform/observer-connections-table": "^0.0.2",
    "@nearform/observer-dht-buckets": "^0.0.2",
    "@nearform/observer-events-table": "^0.0.2",
    "@nearform/observer-samples": "^0.0.2",
    "@nearform/observer-streams-table": "^0.0.2",
    "arraybuffer-loader": "^1.0.8",
    "craco-babel-loader": "^0.1.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-markdown": "^4.2.2",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "create-react-app": "3.2.0",
    "gh-pages": "^2.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "\\.(mock|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__jest__/filePaths.js",
      "\\.js$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    }
  }
}
