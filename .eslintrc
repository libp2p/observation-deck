{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier",
  ],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "commonjs": true,
    "ecmaVersion": 2018,
    "sourceType": "script"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
